{
	"name": "19-web-text-editor",
	"version": "1.0.0",
	"description": "text editor as a progressive web app",
	"main": "server.js",
	"scripts": {
		"start:dev": "concurrently \"cd client && npm run build\" \"cd server && npm run server\" ",
		"start": "npm run build && cd server && node server.js",
		"server": "cd server node server.js --ignore client",
		"build": "cd client && npm run build",
		"install": "cd server && npm i && cd ../client && npm i",
		"client": "cd client && npm start"
	},
	"keywords": [],
	"author": "pbp66",
	"license": "MIT",
	"dependencies": {
		"express": "^4.18.2",
		"if-env": "^1.0.4"
	},
	"devDependencies": {
		"concurrently": "^5.3.0",
		"css-loader": "^6.7.3",
		"css-minimizer-webpack-plugin": "^4.2.2",
		"html-webpack-plugin": "^5.5.0",
		"mini-css-extract-plugin": "^2.7.2",
		"nodemon": "^2.0.4",
		"style-loader": "^3.3.1",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"workbox-webpack-plugin": "^6.5.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pbp66/web-text-editor.git"
	},
	"bugs": {
		"url": "https://github.com/pbp66/web-text-editor/issues"
	},
	"homepage": "https://github.com/pbp66/web-text-editor#readme"
}
